 })
 local Button = MainTab:CreateButton({
    Name = "most uwu antifling ever",
    Callback = function(antifling)
        local Players = game.Players
        local Player = Players.LocalPlayer
        
        if not Player.Character then
            Player.CharacterAdded:Wait()
        end
            
        local function checkPart(v)
            if v.ClassName:lower():find("part") then
                pcall(function()
                    local realPart = v
                    for i, v in pairs(Player.Character:GetDescendants()) do
                        if v.ClassName:lower():find("part") then
                            local nc = Instance.new("NoCollisionConstraint", realPart)
                            nc.Part0 = realPart
                            nc.Part1 = v
                            nc.Enabled = true
                        end
                    end
                end)
            end
        end
        
        local function startOther(player, once)
            local function process()
                if not player.Character then
                    player.CharacterAdded:Wait()
                end
                player.Character:WaitForChild("Head", 3)
                player.Character:WaitForChild("Humanoid", 3)
                local Done = {}
                task.spawn(pcall, function()
                    for i, v in pairs(player.Character:GetDescendants()) do
                        checkPart(v)
                    end
                    player.Character.DescendantAdded:connect(function(v)
                        if not table.find(Done, v) then
                            table.insert(Done, v)
                            checkPart(v)
                        end
                    end)
                end)
            end
            pcall(process)
            if not once then
                player.CharacterAdded:connect(process)
            end
        end
    
        for i, v in pairs(Players:GetPlayers()) do
            if v ~= Player then
                startOther(v, false)
            end
        end
    
        Players.PlayerAdded:connect(startOther, false)
    
        local function ourEnd()
            if not Player.Character then
                Player.CharacterAdded:Wait()
            end
            Player.Character:WaitForChild("Head")
            Player.Character:WaitForChild("Humanoid")
            for i, v in pairs(Players:GetPlayers()) do
                if v ~= Player then
                        for i, v in next, v.Character:GetDescendants() do
                            if v:IsA("NoCollisionConstraint") then
                                v.Parent = nil
                            end
                        end
                    startOther(v, true)
                end
            end
        end
           
        Player.CharacterAdded:connect(ourEnd)
    end,
 })
